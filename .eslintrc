{
  "extends": "eslint:recommended",
  "rules": {
    "indent": [2, 2],
    "quotes": [2, "double"],
    "linebreak-style": [2, "unix"],
    "semi": [2, "always"],
    "no-unused-vars": [1, { "vars": "all", "args": "after-used"}],
    "no-undef": 2,
    "no-console": 1,
    "eqeqeq": 2
  },
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "modules": true
  }
}
